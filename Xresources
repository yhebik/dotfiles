        :<Key>k: up(0.100)\n\
        :<Key>j: down(0.100)\n\
        :<Key>k: up()\n\
        :<Key>l: right()\n\
        :<Key>z: set-shrink-factor(-)\n\
        :<Key>Z: set-shrink-factor(+)\n
